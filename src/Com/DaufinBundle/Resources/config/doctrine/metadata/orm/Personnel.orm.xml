<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Personnel" table="personnel">
    <id name="id" type="bigint" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="matriculePers" type="bigint" column="MATRICULE_PERS" nullable="true"/>
    <field name="nomPers" type="string" column="NOM_PERS" length="56" nullable="true"/>
    <field name="prenomPers" type="string" column="PRENOM_PERS" length="56" nullable="true"/>
    <field name="fonction" type="string" column="FONCTION" length="56" nullable="true"/>
    <field name="telPers" type="string" column="TEL_PERS" length="56" nullable="true"/>
    <field name="gsmPers" type="string" column="GSM_PERS" length="56" nullable="true"/>
    <field name="cinPers" type="string" column="CIN_PERS" length="56" nullable="true"/>
    <field name="dateDebut" type="datetime" column="DATE_DEBUT" nullable="true"/>
    <field name="etatActivitee" type="string" column="ETAT_ACTIVITEE" length="56" nullable="true"/>
  </entity>
</doctrine-mapping>
