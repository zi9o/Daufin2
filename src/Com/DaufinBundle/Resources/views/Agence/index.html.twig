{% extends"ComDaufinBundle::layout.html.twig"%}

{% block _container %}
        <div class="page-head">
                <h2>Liste des Agences</h2>
                <ol class="breadcrumb">
                  <li><a href="#">Home</a></li>
                  <li><a href="#">PARAMETRES</a></li>
                  <li class="active">Liste des Agences</li>
                </ol>
        </div>
{% endblock %}

{% block container%}
    <a href="{{ path('com_agence_new') }}">
        <button class="btn btn-info">
         Nouveau Element        
        </button>
    </a>
<div class="block-flat">        
<div class="table-responsive">
    <table class="table table-bordered" id="datatable">
        <thead>
            <tr>
              
                <th>Code Agence</th>
                <th>Libelle</th>
                <th>Type Agence</th>
                <th>Tel Agence</th>
                <th>Fax Agence</th>
                <th>Adresse</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr> 
                <td><a href="{{ path('com_agence_show', { 'id': entity.id }) }}">{{ entity.codeAgence }}</a></td>
                <td>{{ entity.libelleAg }}</td>
                <td>{{ entity.typeAgence }}</td>
                <td>{{ entity.telAg }}</td>
                <td>{{ entity.faxAg }}</td>
                <td>{{ entity.adresseAg }}</td>
                <td align="center">
                    <a href="{{ path('com_agence_show', { 'id': entity.id }) }}" class="label label-success">
                            <i class="fa   fa-folder-open"></i></a>
                    <a href="{{ path('com_agence_edit', { 'id': entity.id }) }}" class="label label-info">
                        <i class="fa fa-pencil-square-o"></i></a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
</div>


    {% endblock %}
