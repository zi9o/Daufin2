{% extends 'ComDaufinBundle::layout.html.twig' %}

{% block container -%}
    <h1>Site</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Libellesite</th>
                <td>{{ entity.libelleSite }}</td>
            </tr>
            <tr>
                <th>Adressite</th>
                <td>{{ entity.adresSite }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('com_site') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('com_site_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
